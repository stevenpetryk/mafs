{
  "name": "mafs",
  "version": "0.8.1",
  "license": "MIT",
  "author": "Steven Petryk (https://stevenpetryk.com)",
  "homepage": "https://mafs.dev",
  "repository": "github:stevenpetryk/mafs",
  "source": "src/index.tsx",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "start": "cd docs && pnpm dev",
    "build": "rm -rf build && parcel build && cp src/index.css build",
    "build:docs": "cd docs && pnpm gatsby build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "api-report": "api-extractor run",
    "api-report:update": "api-extractor run --local",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.33.7",
    "@parcel/core": "^2.8.2",
    "@parcel/packager-ts": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "@swc/core": "1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-image-snapshot": "^6.1.0",
    "jest-puppeteer": "^6.2.0",
    "parcel": "^2.8.2",
    "prettier": "^2.8.1",
    "puppeteer": "^10.4.0",
    "react": "17",
    "react-dom": "17",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "@use-gesture/react": "^10.2.23",
    "tiny-invariant": "^1.3.1",
    "use-resize-observer": "^9.0.0"
  }
}
