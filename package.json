{
  "name": "mafs",
  "version": "0.17.1",
  "license": "MIT",
  "author": "Steven Petryk (https://stevenpetryk.com)",
  "homepage": "https://mafs.dev",
  "repository": "github:stevenpetryk/mafs",
  "source": "src/index.tsx",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "core.css",
    "font.css"
  ],
  "scripts": {
    "start": "cd docs && pnpm dev",
    "test": "pnpm -s test:unit && pnpm -s test:e2e",
    "test:unit": "jest",
    "test:e2e": "node --loader ts-node/esm scripts/testgen.mts && playwright test",
    "build": "ts-node scripts/build.mts",
    "api-report:update": "pnpm tsc && api-extractor run --local",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepublishOnly": "pnpm test && pnpm lint && pnpm build"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.36.3",
    "@playwright/experimental-ct-react": "^1.36.2",
    "@playwright/test": "^1.36.2",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/jest-image-snapshot": "^6.2.0",
    "@types/katex": "^0.16.2",
    "@types/node": "^20",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.6.2",
    "jest-image-snapshot": "^6.2.0",
    "prettier": "^3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.6",
    "@swc/helpers": "^0.5",
    "@use-gesture/react": "^10.2.27",
    "computer-modern": "^0.1.2",
    "katex": "^0.16",
    "tiny-invariant": "^1.3.1",
    "use-resize-observer": "^9"
  }
}
