{
  "name": "mafs",
  "version": "0.15.1",
  "license": "MIT",
  "author": "Steven Petryk (https://stevenpetryk.com)",
  "homepage": "https://mafs.dev",
  "repository": "github:stevenpetryk/mafs",
  "source": "src/index.tsx",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "core.css",
    "font.css"
  ],
  "scripts": {
    "start": "cd docs && pnpm dev",
    "test": "pnpm -s test:unit && pnpm -s test:e2e",
    "test:unit": "jest",
    "test:e2e": "ts-node scripts/testgen.mts && playwright test",
    "build": "ts-node scripts/build.mts",
    "api-report:update": "pnpm tsc && api-extractor run --local",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepublishOnly": "pnpm test && pnpm lint && pnpm build"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.33.7",
    "@playwright/experimental-ct-react": "^1.29.2",
    "@playwright/test": "^1.29.2",
    "@swc/core": "1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "esbuild": "^0.17.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "jest": "^29.3.1",
    "jest-image-snapshot": "^6.1.0",
    "prettier": "^2.8.1",
    "react": "17",
    "react-dom": "17",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.6",
    "@swc/helpers": "^0.4.14",
    "@use-gesture/react": "^10.2.23",
    "computer-modern": "^0.1.2",
    "tiny-invariant": "^1.3.1",
    "use-resize-observer": "^9.0.0"
  }
}
