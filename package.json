{
  "name": "mafs",
  "version": "0.4.3",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cd docs && yarn install && yarn gatsby develop",
    "build": "rm -rf build && yarn -s build:code & yarn -s build:types",
    "build:code": "node esbuild.js",
    "build:types": "yarn tsc && yarn api-extractor run && rm -rf build/type-declarations",
    "build:docs": "cd docs && yarn install && yarn gatsby build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "postinstall": "chmod +x prepublish.sh",
    "publish": "./prepublish.sh && cd build && npm publish"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@microsoft/api-extractor": "^7.18.17",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.2",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/node": "^14",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "esbuild": "^0.12",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^7",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "jest-image-snapshot": "^4.5.1",
    "jest-puppeteer": "^6.0.0",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@use-gesture/react": "^10",
    "tiny-invariant": "^1",
    "use-resize-observer": "^7 || ^8",
    "vec-la": "^1.5.0"
  }
}
